{
  "name": "personal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@vercel/node": "^2.15.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,CRA\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "react-scripts start",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "wait-on -l api/index.js && nodemon api/index.js",
    "build": "npm run build:server && npm run build:client",
    "build:client": "react-scripts build",
    "build:server": "webpack --config webpack.config.server.js",
    "build:vercel": "npm run build:server && npx vercel build",
    "deploy:vercel": "npm run build:server && npx vercel deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "copy-webpack-plugin": "^11.0.0",
    "typescript": "4.9.5"
  }
}
